import java.util.*;
public class Solution {     
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
       long arr[]=new long[5];
       for(int i=0;i<arr.length;i++){
           arr[i]=sc.nextInt();
       } 
          Arrays.sort(arr);
     long x=0;
        long y=0;
     
        for(int i=0;i<4;i++){
            x=x+arr[i];
        }
        for(int i=1;i<5;i++){
            y=y+arr[i];
        }
        System.out.println(x+" "+y);
    }
    }
